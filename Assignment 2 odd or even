#include <stdio.h>

int main()
{
    int i = 1, oddsum = 0, evensum = 0;
    while (i <= 10)
    {
        printf("%d\n", i);
        if (i % 2 == 0)
        {
            evensum = evensum + i;
        }
        else
        {
            oddsum = oddsum + i;
        }
        i++;
    }
    printf("odd sum = %d\n", oddsum);
    printf("even sum = %d\n", evensum);
    return 0;
}
